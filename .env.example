# DCI Research Agent - Environment Variables
# Copy to .env and fill in your API keys

# OpenAI API Key (used for PageIndex tree generation and search)
OPENAI_API_KEY=sk-your-openai-key-here

# Anthropic API Key (used for domain agents and synthesis)
ANTHROPIC_API_KEY=sk-ant-your-anthropic-key-here

# PageIndex API Key (optional - for hosted PageIndex service)
PAGEINDEX_API_KEY=

# Model Configuration (defaults shown)
PAGEINDEX_MODEL=gpt-4o
ROUTER_MODEL=gpt-4o-mini
AGENT_MODEL=claude-sonnet-4-20250514
SYNTHESIZER_MODEL=claude-sonnet-4-20250514

# Logging
LOG_LEVEL=INFO
